<h1>Listing of Students</h1>
 
<%= link_to 'New Student', new_student_path %> 
 
<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
  </tr>
 
  <% @students.each do |student| %>
    <tr>
      <td><%= student.first_name %></td>
      <td><%= student.last_name %></td>
      <td><%= link_to 'Show', student_path(student) %></td>
      <td><%= link_to 'Edit', edit_student_path(student) %></td>
      <td><%= link_to 'Delete', student_path(student),
              method: :delete,
              data: { confirm: 'Are you sure you want to delete the student?' } %></td>
    </tr>
    </tr>
  <% end %>
</table>



<%= link_to 'Welcome Page', root_path %>
</div>